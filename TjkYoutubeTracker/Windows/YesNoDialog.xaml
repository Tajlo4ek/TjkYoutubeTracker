<Window x:Class="TjkYoutubeTracker.Windows.YesNoDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TjkYoutubeTracker.Windows"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="dialog" Height="150.79" Width="465.5">
    <Grid>
        <Button x:Name="btnNo" Content="No" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="BtnNo_Click"/>
        <Button x:Name="btnYes" Content="Yes" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Click="BtnYes_Click"/>
        <Label x:Name="labelAsk"  HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="440"/>
    </Grid>
</Window>
